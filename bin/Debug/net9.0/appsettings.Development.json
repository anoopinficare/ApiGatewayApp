{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Hosting": "Information",
      "Microsoft.AspNetCore.Routing": "Debug",
      "Microsoft.Extensions.Http": "Debug",
      "System.Net.Http.HttpClient": "Debug",
      "Ocelot": "Trace",
      "Ocelot.Requester": "Trace",
      "Ocelot.DownstreamRouteFinder": "Trace",
      "Ocelot.Middleware": "Trace",
      "Ocelot.RateLimiting": "Trace",
      "Ocelot.Authentication": "Trace",
      "Ocelot.Authorization": "Trace",
      "Ocelot.LoadBalancer": "Trace",
      "Ocelot.ServiceDiscovery": "Trace"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss.fff ",
      "LogToStandardErrorThreshold": "Debug"
    }
  }
}
